pipeline {
    agent any

    stages {
        stage("SCM Checkout") {
            steps {
                git branch: 'main',
                    credentialsId: "github_access",                
                    url: "https://github.com/rajasekhar2304/test_iac.git"            
            }
        }
        stage('Clean Up') {
            steps {
                sh "sh script.sh"
            }
        }
    }
}